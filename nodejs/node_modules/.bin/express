#!/bin/sh
basedir=$(dirname "$(echo "$0" | sed -e 's,\\,/,g')")

case `uname` in
    *CYGWIN*|*MINGW*|*MSYS*) basedir=`cygpath -w "$basedir"`;;
esac

if [ -x "$basedir/node" ]; then
  "$basedir/node"  "$basedir/../_express-generator@4.16.1@express-generator/bin/express-cli.js" "$@"
  ret=$?
else 
  node  "$basedir/../_express-generator@4.16.1@express-generator/bin/express-cli.js" "$@"
  ret=$?
fi
exit $ret
